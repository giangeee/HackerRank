    if len(set(s1).intersection(set(s2))) != 0:
        print("YES")
        # return "YES"
    else:
        print("NO")
        # return "NO"

if __name__ == '__main__':
    q = int(input())

    for q_itr in range(q):
        s1 = input()

        s2 = input()

        result = twoStrings(s1, s2)
